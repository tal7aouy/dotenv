{
    "name": "tal7aouy/dotenv",
    "description": "editing dotenv files in an easy way",
    "keywords": [
        "phpdotenv",
        "dotenv",
        "dotenv-editor",
        "env"
    ],
    "homepage": "https://github.com/tal7aouy/dotenv",
    "license": "MIT",
    "authors": [
        {
            "name": "Mhammed Talhaouy",
            "email": "tal7aouy@gmail.com",
            "role": "PHP Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tal7aouy\\Dotenv\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tal7aouy\\Dotenv\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "vendor/bin/php-cs-fixer fix --config=./.php-cs-fixer.dist.php",
        "test": [
            "@composer install",
            "vendor/bin/phpunit"
          ]
    },

    "config": {
        "sort-packages": true
    }
}
